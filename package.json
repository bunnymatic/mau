{
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.0",
    "eslint": "^7.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.0",
    "prettier": "^2.0.0",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "eslint-fix": "eslint --fix app/**/*.js",
    "eslint-lint": "eslint app/**/*.js",
    "lint": "npm run eslint-lint && npm run prettier-lint",
    "lint-fix": "npm run eslint-fix && npm run prettier-fix",
    "prettier-fix": "prettier --write app/**/*.js app/**/*.scss **/*.json",
    "prettier-lint": "prettier --list-different app/**/*.js app/**/*.scss **/*.json",
    "test": "jest app",
    "test-watch": "jest app --watch"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@google/markerclustererplus": "^5.0.3",
    "@rails/ujs": "^6.0.2-1",
    "@rails/webpacker": "^5.0.0",
    "angular": "^1.8.0",
    "angular-animate": "^1.8.0",
    "angular-mocks": "^1.8.0",
    "angular-resource": "^1.8.0",
    "angular-sanitize": "^1.8.0",
    "angular-ui-utils": "^0.1.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "browserslist-useragent": "^3.0.2",
    "c3": "^0.7.11",
    "core-js": "^3.6.4",
    "d3": "^5.14.2",
    "datatables": "^1.10.18",
    "html-loader": "^1.0.0",
    "humps": "^2.0.1",
    "import-glob-loader": "^1.1.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.5.0",
    "jquery-ui": "^1.12.1",
    "luxon": "^1.25.0",
    "ng-device-detector": "^5.1.3",
    "ng-dialog": "^1.4.0",
    "pickadate": "3.6.2",
    "postcss-cssnext": "^3.1.0",
    "purecss": "^2.0.0",
    "re-tree": "^0.1.7",
    "regenerator-runtime": "^0.13.5",
    "resolve-url-loader": "^3.1.1",
    "select2": "^4.0.13",
    "selectize": "^0.12.6",
    "spin.js": "^4.1.0",
    "webpack-merge": "^4.2.2",
    "zxcvbn": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "html"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.html$": "<rootDir>/jest/support/html_loader.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(spin.js)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/config/"
    ],
    "setupFiles": [
      "./jest/setupFiles.js"
    ],
    "moduleNameMapper": {
      "@js/(.*)$": "<rootDir>/app/webpack/js/$1",
      "@angularjs/(.*)$": "<rootDir>/app/webpack/angularjs/$1",
      "@components/(.*)$": "<rootDir>/app/webpack/angularjs/components/$1",
      "@services/(.*)$": "<rootDir>/app/webpack/angularjs/services/$1",
      "@support/(.*)$": "<rootDir>/jest/support/$1",
      "@fixtures/(.*)$": "<rootDir>/spec/fixtures/$1"
    }
  }
}
