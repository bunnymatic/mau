# delete
script/destroy scaffold Studio Artist User Session

#generate new schema
script/generate scaffold Studio \
name:string \
street:string \
city:string \
state:string \
zip:integer \
url:string 

./script/generate authenticated artist sessions —include-activation —stateful —rspec —skip-migration —skip-routes —old-passwords

rake db:drop && rake db:create && rake db:migrate

