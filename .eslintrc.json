{
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "$": true,
    "angular": true,
    "google": true,
    "jQuery": true,
    "MAU": true,
    "module": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": ["standard", "prettier"],
  "rules": {
    "no-duplicate-imports": ["error", { "includeExports": true }],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "prettier/prettier": 2
  },
  "extends": ["eslint:recommended", "prettier"]
}
