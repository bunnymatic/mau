<div class="map-os-switch">
  <form id="map_osswitcher" action="">
    <% params.each do |k,v| -%>
    <% if !['osonly','all'].include? k -%>
    <input name="<%= k %>" id="<%= k %>" type="hidden" value="<%= v %>"/>
    <% end -%>
    <% end -%>
    <% if @os_only -%>
    <input name="all" id="map_cb" type="checkbox" checked="checked"/>
    <% else -%>
    <input name="osonly" id="map_cb" type="checkbox"/>
    <% end -%>
    (showing only open studios artists)
  </form>
</div>
