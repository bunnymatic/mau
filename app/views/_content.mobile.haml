%table.tbl-content{:cellpadding => "0", :cellspacing => "0", :id => "mau-content-html"}
  %tr.nextrow
    - if cols == 2
      %td.lcol.graybg{:colspan => '2'}
        = render :partial => '/feeds/feed' 
      %td.vstripe
        %td.rcol{:colspan => '2'}
          / content.html
          = yield
    - elsif cols == 1
      %td.singlecolumn{:colspan => "3"}
        = yield
