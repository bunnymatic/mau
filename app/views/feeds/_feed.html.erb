<% if logged_in? && request.request_uri != getinvolved_path -%>
<div class="get-involved">
<center><a title="Get Involved!"  href="/getinvolved"><img border="0" title="Get Involved!" src="/images/get_involved_btn.png"/></a></center>
</div>
<% else -%>
<div class="feed-studios">
<%   if @studios -%>
<div class="feed-sxn-hdr">MAU Studios</div>
<div class="feed-studio-list">
<%     @studios.each do |s| -%>
<div><%= link_to s.name, "/studios/%d" % s.id %></div>
<%     end -%>
<%   end -%>
</div>
</div>
<% end -%>
<div id='feed_div'> 
<div class="feed-sxn-hdr" >Fetching Feeds... 
  <div align="center"><img src="/images/spinner32.gif" alt=""/></div>
</div>
<img src="/images/pix.gif" height="320" width="1" alt=""/>
</div> 
<div id="feed_request">
<a href="javascript:MAU.mailer('www','missionartistsunited.org','My Favorite SF Art Feed');">Recommend a feed we should add to this list.</a>
