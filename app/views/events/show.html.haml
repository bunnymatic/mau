.events
  = render :partial => '/flash_notice_error'
  .events_link
    = link_to "View Events List &raquo;", events_path
  .calendar_link
    = link_to "View Events Calendar &raquo;", calendar_path(:month => Time.zone.now.month, :year => Time.zone.now.year)
  = render :partial => 'event', :locals => { :event => @event, :force => true }
