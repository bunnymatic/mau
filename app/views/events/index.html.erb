<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Tweet</th>
    <th>Venue</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Starttime</th>
    <th>Endtime</th>
    <th>Url</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%=h event.title %></td>
    <td><%=h event.description %></td>
    <td><%=h event.tweet %></td>
    <td><%=h event.venue %></td>
    <td><%=h event.street %></td>
    <td><%=h event.city %></td>
    <td><%=h event.state %></td>
    <td><%=h event.zip %></td>
    <td><%=h event.starttime %></td>
    <td><%=h event.endtime %></td>
    <td><%=h event.url %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event', new_event_path %>