<h1>Listing cms_documents</h1>

<table>
  <tr>
  </tr>

<% @cms_documents.each do |cms_document| %>
  <tr>
    <td><%= cms_document.page %></td>
    <td><%= cms_document.section %></td>
    <td><%= link_to 'Show', cms_document %></td>
    <td><%= link_to 'Edit', edit_cms_document_path(cms_document) %></td>
    <td><%= link_to 'Destroy', cms_document, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

